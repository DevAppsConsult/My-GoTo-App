<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.mygotoservices.mygotoandroid">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-feature
        android:name="android.hardware.location.network"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.location.gps"
        android:required="false" />

    <application
        android:name=".AppController.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme"
        tools:replace="android:theme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_places_key" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" />
        <activity android:name=".Artisan.ArtisanLoginActivity" />
        <activity android:name=".Merchant.MerchantLoginActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ConfirmCodeActivity" />
        <activity
            android:name=".SingleProductActivity"
            android:label="@string/title_activity_single_product"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MyMaterialTheme" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.mygotoservices.mygotoandroid.MainActivity" />

        <activity
            android:name=".SingleServiceActivity"
            android:label="@string/title_activity_single_service"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MyMaterialTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mygotoservices.mygotoandroid.MainActivity" />
        </activity>
        <activity
            android:name=".BookServiceActivity"
            android:label="@string/title_activity_book_service"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MyMaterialTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mygotoservices.mygotoandroid.MainActivity" />
        </activity>
        <activity
            android:name=".CheckOutActivity"
            android:label="@string/title_activity_check_out"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MyMaterialTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mygotoservices.mygotoandroid.MainActivity" />
        </activity>
        <activity
            android:name=".ThankYouActivity"
            android:label="@string/title_activity_thank_you"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MyMaterialTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mygotoservices.mygotoandroid.MainActivity" />
        </activity>
        <activity
            android:name=".MobileMoneyPaymentActivity"
            android:label="@string/title_activity_mobile_money_payment"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MyMaterialTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mygotoservices.mygotoandroid.MainActivity" />
        </activity>
        <activity
            android:name=".CardPaymentActivity"
            android:label="@string/title_activity_card_payment"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name=".TransactionsHistoryActivity"
            android:label="@string/title_activity_transactions_history"
            android:theme="@style/MyMaterialTheme" />
        <activity android:name=".ForgotPasswordActivity" />
        <activity android:name=".OtherPaymentActivity" />
        <activity android:name=".MyBookingsActivity" />
        <activity android:name=".MyFavouriteActivity" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mygotoservices.mygotoandroid.MainActivity" />
        </activity>
        <activity android:name=".MyAccountActivity" />
        <activity
            android:name=".LocationPickerActivity"
            android:label="@string/title_activity_location_picker" />
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MyMaterialTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mygotoservices.mygotoandroid.MainActivity" />
        </activity>
        <activity
            android:name=".CustomerCareActivity"
            android:label="@string/title_activity_customer_care"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MyMaterialTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mygotoservices.mygotoandroid.MainActivity" />
        </activity>
        <activity
            android:name=".FaqActivity"
            android:label="@string/title_activity_faq"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MyMaterialTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mygotoservices.mygotoandroid.MainActivity" />
        </activity>
        <activity
            android:name=".ArtisanRequestActivity"
            android:label="@string/title_activity_artisan_request"
            android:parentActivityName=".MainActivity"
            android:theme="@style/MyMaterialTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mygotoservices.mygotoandroid.MainActivity" />
        </activity>
    </application>

</manifest>